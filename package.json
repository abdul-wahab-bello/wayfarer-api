{
  "name": "wayfarer-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "babel ./src -d dist && npm run docs",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "dev": "DEBUG=wayfarer-api:* nodemon --exec babel-node --presets=@babel/env -- src/server",
    "docs": "snowboard html -o public/index.html documentation.apib",
    "start": "DEBUG=wayfarer-api:* node dist/server",
    "test": "mocha --require @babel/polyfill --require @babel/register",
    "test:watch": "mocha --require @babel/polyfill --require @babel/register --watch"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "snowboard": "^2.0.6"
  }
}
